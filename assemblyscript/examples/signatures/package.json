{
  "scripts": {
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm --runtime stub",
    "asbuild:small": "asc assembly/index.ts -b build/small.wasm -t build/small.wat -O3z --runtime stub",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat -O3 --runtime stub",
    "asbuild": "npm run asbuild:optimized",
  },
  "devDependencies": {
    "assemblyscript": "github:AssemblyScript/assemblyscript"
  }
}
